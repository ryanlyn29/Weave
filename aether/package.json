{
  "name": "aether-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:web": "cd apps/web && npm run dev",
    "dev:server": "cd apps/server && go run main.go",
    "build:web": "cd apps/web && npm run build",
    "install:web": "cd apps/web && npm install"
  },
  "description": "AETHER - Real-time Multiplayer Generative 3D Environment"
}


