{
  "name": "storysprint",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:frontend": "next dev",
    "dev:backend": "powershell -Command \"Set-Location backend-java; $env:JAVA_HOME = if ($env:JAVA_HOME) { $env:JAVA_HOME } else { Split-Path (Split-Path (Get-Command java).Source) }; if (Test-Path 'mvnw.cmd') { & '.\\.\\mvnw.cmd' spring-boot:run } else { Write-Host 'ERROR: Maven wrapper not found. Run: npm run setup:maven' }\"",
    "setup:maven": "powershell -Command \"New-Item -ItemType Directory -Path 'backend-java\\.mvn\\wrapper' -Force | Out-Null; Invoke-WebRequest -Uri 'https://repo.maven.apache.org/maven2/org/apache/maven/wrapper/maven-wrapper/3.2.0/maven-wrapper-3.2.0.jar' -OutFile 'backend-java\\.mvn\\wrapper\\maven-wrapper.jar'\"",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@auth/firebase-adapter": "^1.6.0",
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/s3-request-presigner": "^3.515.0",
    "@octokit/rest": "^20.1.1",
    "@react-spring/web": "^9.7.5",
    "@stripe/stripe-js": "^2.4.0",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "elevenlabs-node": "^1.2.0",
    "firebase": "^10.12.0",
    "firebase-admin": "^11.11.0",
    "lucide-react": "^0.562.0",
    "next": "^14.2.0",
    "next-auth": "^4.24.5",
    "openai": "^4.28.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-tinder-card": "^1.6.2",
    "stripe": "^14.21.0",
    "swr": "^2.3.8",
    "tailwind-merge": "^3.4.0",
    "typescript": "^5.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.0"
  }
}
